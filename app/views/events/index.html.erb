<form id="search-form">
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "search" %>
</form>

<hr>

<div class="event">
  <% @events.each do |event| %>
    <a href="<%= event_path(event) %>">
      <%= event.event_name %></br>
      <%= event.full_street_address %>
    </a>
    <hr>
  <% end %>
</div>
